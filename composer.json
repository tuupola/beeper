{
    "name": "tuupola/beeper",
    "type": "library",
    "description": "Generic paginator for PHP 5.4+",
    "keywords": ["pager", "pagination", "paginator"],
    "homepage": "https://github.com/tuupola/beeper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "http://www.appelsiini.net/"
        }
    ],
    "require": {
        "php" : "^5.4 || ^7.0",
        "tuupola/witchcraft": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.5 || ^6.5",
        "squizlabs/php_codesniffer": "^1.5",
        "league/fractal": "^0.15.0"
    },
    "suggest": {
        "vlucas/spot2": "Allows running Spot tests"
    },
    "autoload": {
        "psr-4": {
            "Beeper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beeper\\Test\\": "test"
        }
    }
}
